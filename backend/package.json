{
  "name": "credit-card-suggestor-backend",
  "version": "1.0.0",
  "main": "dist/server.js",
  "description": "Credit Card Recommendation System Backend - AI-powered PDF analysis and personalized card suggestions",
  "scripts": {
    "dev": "ts-node-dev -r tsconfig-paths/register --respawn --transpile-only --project tsconfig.json src/server.ts",
    "build": "tsc",
    "start": "node -r tsconfig-paths/register dist/server.js",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write .",
    "prisma:generate": "prisma generate --schema src/database/schema.prisma",
    "prisma:migrate:dev": "prisma migrate dev --schema src/database/schema.prisma",
    "prisma:migrate:deploy": "prisma migrate deploy --schema src/database/schema.prisma",
    "prisma:studio": "prisma studio --schema src/database/schema.prisma",
    "db:seed": "ts-node -r tsconfig-paths/register src/database/seeds/index.ts",
    "db:reset": "prisma migrate reset --force --schema src/database/schema.prisma && npm run db:seed",
    "typecheck": "tsc --noEmit",
    "quality": "npm run prisma:generate && npm run typecheck && npm run lint:fix && npm run format && npm run build",
    "quality:check": "npm run prisma:generate && npm run typecheck && npm run lint && npm run build"
  },
  "keywords": [
    "credit-cards",
    "ai",
    "gemini",
    "pdf-processing",
    "recommendation-engine",
    "typescript",
    "express"
  ],
  "author": "Credit Card Suggestor Team",
  "license": "ISC",
  "dependencies": {
    "@google/generative-ai": "^0.21.0",
    "@prisma/client": "^6.11.1",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.0",
    "express": "^5.1.0",
    "express-http-context": "^2.0.1",
    "express-rate-limit": "^7.5.1",
    "helmet": "^8.1.0",
    "http-status-codes": "^2.3.0",
    "joi": "^17.13.3",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^4.2.0",
    "pdf-parse": "^1.1.1",
    "pg": "^8.16.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "tsconfig-paths": "^4.2.0",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "winston-datadog-transport": "^1.0.2",
    "zod": "^4.0.2"
  },
  "devDependencies": {
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/joi": "^17.2.2",
    "@types/morgan": "^1.9.10",
    "@types/multer": "^1.4.12",
    "@types/node": "^24.0.12",
    "@types/node-cron": "^3.0.11",
    "@types/pdf-parse": "^1.1.4",
    "@types/pg": "^8.11.10",
    "@types/supertest": "^6.0.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.36.0",
    "@typescript-eslint/parser": "^8.36.0",
    "eslint": "^9.30.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.1",
    "globals": "^16.3.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "pgmock": "^1.0.3",
    "prettier": "^3.6.2",
    "prisma": "^6.11.1",
    "supertest": "^7.1.3",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.36.0"
  }
}
